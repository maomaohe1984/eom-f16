size(500,500);

// fill and stroke color
// for all my ellipses
fill(0,255,0); //green
stroke(0,200,0); //darker-green

// x,y coord     width,height
ellipse(250,250, 100,50);
ellipse(260,260, 100,50);
ellipse(270,270, 100,50);

// a red circle!
// it still has the green stroke
// outline from above
fill(255,0,0);
ellipse(300,300, 50,50);

